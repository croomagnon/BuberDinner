{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost:5433;User ID=postgres;Password=postgres"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "WriteTo": [
        {
          "Name": "Async",
          "Args": {
            "configure": [
              {
                "Name": "Console",
                "Args": {
                  "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message}{NewLine}{Exception}"
                }
              }
            ]
          }
        }
      ],
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "Microsoft.AspNetCore.Hosting.Internal.WebHost": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "JwtSettings": {
    "Secret": "BuberDinnerSecret",
    "ExpireMinutes": 60,
    "Issuer": "BuberDinner",
    "Audience": "BuberDinner"
  }
}
